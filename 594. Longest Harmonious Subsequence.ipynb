   "source": [
    "class Solution:\n",
    "    def findLHS(self, nums: List[int]) -> int:\n",
    "        count = collections.Counter(nums)\n",
    "        return max([count[n] + count[n + 1] for n in count if n + 1 in count] or [0])"
   ]
  }
