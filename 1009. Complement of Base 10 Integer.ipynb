   "source": [
    "class Solution:\n",
    "    def bitwiseComplement(self, n: int) -> int:\n",
    "        return (int((bin(n)[2:]).replace('1', '2').replace('0','1').replace('2','0'),2))\n",
    "\n",
    "        return (int((bin(n)[2:]).translate(str.maketrans(\"01\", \"10\")),2))"
   ]
  }
