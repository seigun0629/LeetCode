   "source": [
    "class Solution:\n",
    "    def firstUniqChar(self, s: str) -> int:\n",
    "        char_count={}\n",
    "\n",
    "        for char in s:\n",
    "            char_count[char] = char_count.get(char,0)+1\n",
    "\n",
    "        for i,char in enumerate(s):\n",
    "            if char_count[char] == 1:\n",
    "                return i\n",
    "        return -1"
   ]
  }
