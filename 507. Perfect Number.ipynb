   "source": [
    "class Solution:\n",
    "    def checkPerfectNumber(self, num: int) -> bool:\n",
    "        if num==1: return False\n",
    "        answer = 1\n",
    "        i = 2\n",
    "        while i*i<num:\n",
    "            if num%i==0: answer+=i+num//i\n",
    "            i+=1\n",
    "        if i*i==num: answer+=i\n",
    "        return answer==num"
   ]
  }
