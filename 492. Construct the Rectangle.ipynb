    "class Solution:\n",
    "    def constructRectangle(self, area: int) -> List[int]:\n",
    "        l=[area,1]\n",
    "        d=l[0]-l[1]\n",
    "        i=2\n",
    "        x=0\n",
    "        while i*i<=area:\n",
    "            if area%i==0:\n",
    "                x=area//i\n",
    "                l=[x,i]\n",
    "            i+=1\n",
    "        return l\n"
   ]
  }
