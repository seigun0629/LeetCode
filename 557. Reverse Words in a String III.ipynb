   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "\n",
    "        words = s.split()\n",
    "        reversed_words = [word[::-1] for word in words]\n",
    "\n",
    "\n",
    "        return ' '.join(reversed_words)"
   ]
  }
