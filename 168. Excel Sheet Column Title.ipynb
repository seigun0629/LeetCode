   "source": [
    "class Solution:\n",
    "    def convertToTitle(self, columnNumber: int) -> str:\n",
    "        result = []\n",
    "        while columnNumber:\n",
    "            columnNumber, remainder = divmod(columnNumber - 1, 26)\n",
    "            result.append(chr(65 + remainder))\n",
    "        return ''.join(reversed(result))"
   ]
  }
