   "source": [
    "class Solution:\n",
    "    def getIntersectionNode(self, headA: ListNode, headB: ListNode) -> ListNode:\n",
    "        a, b = headA, headB\n",
    "        while (a != b):\n",
    "            a = headB if not a else a.next\n",
    "            b = headA if not b else b.next\n",
    "        return a"
   ]
  }
