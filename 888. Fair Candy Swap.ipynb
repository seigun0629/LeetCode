   "source": [
    "class Solution:\n",
    "    def fairCandySwap(self, aliceSizes: List[int], bobSizes: List[int]) -> List[int]:\n",
    "        a=set(aliceSizes)\n",
    "        b=set(bobSizes)\n",
    "        c=(sum(aliceSizes)-sum(bobSizes))/2\n",
    "        for i in a:\n",
    "            if i-c in b:\n",
    "                return [i,i-c]"
   ]
  }
