   "source": [
    "import pandas as pd\n",
    "\n",
    "def find_customer_referee(customer: pd.DataFrame) -> pd.DataFrame:\n",
    "    return (\n",
    "        customer\n",
    "        .assign(\n",
    "            referee_id=lambda frame: frame.referee_id.fillna(0)\n",
    "        )\n",
    "        .query(\"referee_id != 2\")\n",
    "        [[\"name\"]]\n",
    "    )\n",
    "    "
   ]
  }
